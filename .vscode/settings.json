{
    "files.eol": "\n",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "eslint.validate": ["javascript"],
    "rules" : {
        "no-console": "off",
        "quotes": [
            "error",
            "double"
        ],
        "endOfLine": "auto",
        "comma-dangle": ["error", "always-multiline"],
    "linebreak-style": ["error", "unix"]
    }
}